<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow reading files (proto files from user-specified directories) -->
    <key>com.apple.security.files.user-selected.read-only</key>
    <true/>
    
    <!-- Disable library validation to allow loading Python C extensions 
         (protobuf, rapidfuzz, etc. may be compiled native extensions) -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Allow unsigned executable memory for Python bytecode execution -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
</dict>
</plist>

